{
  "name": "simple-angular-template",
  "version": "1.0.0",
  "description": "FrontEnd FestiVer",
  "start": 8000,
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss ./styles/main.scss   ./styles/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "concurrently --kill-others 'npm run watch-css' 'autoreload-server ./ 8800'"
  },
